<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper  namespace="ReviewMapper">
	<select id="list" resultType="com.example.domain.MBasketHVO">
		select * from mbasketh
	</select>
	<insert id="reviewinsert">
		insert into mbasketh(mhid,mhrecent)
		values(#{mhid},#{mhrecent})
	</insert>
</mapper>

